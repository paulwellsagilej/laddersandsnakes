			<button id="2d4e45e8" text="Bass clef" />
			<button correct="true" id="4d9c1f14" text="Treble Clef" />
		</response>
		<hint />
	</question>

	<question category="Theory/Rests" id="85061bf8">
		<prompt text="Which of these are rests?" />
		<response format="checkboxes-then-submit">
			<button correct="true" id="da24377f">
				<lilyPond image="true" template="treble.ly">
					<insert content="r4" />
					<insert content="r8" />
				</lilyPond>
			</button>
			<button correct="true" id="88392a92">
				<lilyPond image="true" template="treble.ly">
					<insert content="r1" />
					<insert content="r2" />
				</lilyPond>
			</button>
			<button id="f50e95e4">
				<lilyPond image="true" template="treble.ly">
					<insert content="c'1" />
					<insert content="c'2" />
					<insert content="c'4" />
					<insert content="c'8" />
				</lilyPond>
			</button>
		</response>
		<hint />
	</question>

	<question category="Theory/Pitch" id="50e1bb8d">
		<prompt text="Arrange these notes in ascending order" />
