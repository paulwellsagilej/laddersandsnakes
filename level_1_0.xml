<level height="1.0" length="10" win="15" hintCost="3">
	<question category="General/History" id="1ff1bfee">
		<prompt text="Who composed Beethoven's fifth symphony?">
			<lilyPond notes="true" template="treble.ly">
				<insert content="g'16 g g ees2 f16 f f d2" />
			</lilyPond>
		</prompt>
		<response format="single-pick-to-submit">
			<button correct="true" id="f95243c3" text="Beethoven" />
			<button id="7891e4fe" text="Mozart" />
			<button id="61eb24ed" text="Bach" />
		</response>
		<hint />
	</question>

	<question category="Theory/notes" id="b25bd3a2">
		<prompt text="A period in the music when we play no notes is called a..." >
			<lilyPond notes="true" template="stave.ly">
				<insert content="r2 r4 r8 r8" />
			</lilyPond>
		</prompt>
		<response format="single-pick-to-submit">
			<button correct="true" id="589e721e" text="rest" />
			<button id="a1eb1f1f" text="break" />
			<button id="2b433936" text="pause" />
			<button id="1043c753" text="holiday" />
		</response>
		<hint />
	</question>

	<question category="Playing/Reading" id="e00c4484">
		<prompt text="What is the name of the symbol on the left?">
			<lilyPond image="true" template="treble.ly">
				<insert content="c'" />
			</lilyPond>
		</prompt>
		<response format="single-pick-to-submit">
