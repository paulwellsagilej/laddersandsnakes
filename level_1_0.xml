			<button correct="true" id="8644a0a1">
				<lilyPond image="true" lights="true" notes="true" template="treble.ly">
					<insert content="c'" />
				</lilyPond>
			</button>
			<button correct="true" id="7ff7e3a2">
				<lilyPond image="true" lights="true" notes="true" template="treble.ly">
					<insert content="e'" />
				</lilyPond>
			</button>
			<button correct="true" id="b1bc3bfe">
				<lilyPond image="true" lights="true" notes="true" template="treble.ly">
					<insert content="g'" />
				</lilyPond>
			</button>
		</response>
		<hint />
	</question>

	<question category="Playing/Reading" id="deffd2d5">
